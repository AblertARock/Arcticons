<appfilter>

	<!-- AyuGram -->
	<item component="ComponentInfo{com.radolyn.ayugram/com.exteragram.messenger.DefaultIcon}" drawable="ayugram"/>

	<!-- BCR -->
	<item component="ComponentInfo{com.chiller3.bcr/com.chiller3.bcr.settings.SettingsActivity}" drawable="bcr"/>

	<!-- BCR-GUI -->
	<item component="ComponentInfo{com.github.nicorac.bcrgui/com.github.nicorac.bcrgui.MainActivity}" drawable="bcrgui"/>

	<!-- Find Device -->
	<item component="ComponentInfo{com.google.android.apps.adm/com.google.android.apps.adm.activities.MainActivity}" drawable="find_device"/>

	<!-- Fit -->
	<item component="ComponentInfo{com.google.android.apps.fitness/com.google.android.apps.fitness.welcome.WelcomeActivity}" drawable="fit"/>

	<!-- Kotak811 -->
	<item component="ComponentInfo{com.kotak811mobilebankingapp.instantsavingsupiscanandpayrecharge/com.kotak811mobilebankingapp.instantsavingsupiscanandpayrecharge.MainActivity}" drawable="kotak811"/>

	<!-- Lens -->
	<item component="ComponentInfo{com.google.ar.lens/com.google.vr.apps.ornament.app.lens.LensLauncherActivity}" drawable="lens"/>

	<!-- MRepo -->
	<item component="ComponentInfo{com.sanmer.mrepo/com.sanmer.mrepo.ui.activity.MainActivity}" drawable="mrepo"/>

	<!-- OnePlus Health -->
	<item component="ComponentInfo{com.oneplus.health.international/com.heytap.health.launch.LaunchActivity}" drawable="oneplus_health"/>

	<!-- Reading mode -->
	<item component="ComponentInfo{com.google.android.accessibility.reader/com.google.android.accessibility.reader.settings.ReaderActivity}" drawable="reading_mode"/>

	<!-- X -->
	<item component="ComponentInfo{com.twitter.android/com.twitter.android.StartActivity}" drawable="x"/>

</appfilter>